      "medium",
      "high",
      "critical",
      "urgent"
    ],
    "level": [
      "debug",
      "info",
      "warning",
      "error",
      "critical"
    ],
    "mode": [
      "auto",
      "manual",
      "hybrid",
      "disabled"
    ],
    "role": [
      "read",
      "write",
      "admin",
      "owner",
      "viewer"
